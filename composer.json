{
    "name": "dragonwize/dw-log-bundle",
    "description": "Highly opinionated log storage and viewer for Symfony in low traffic use cases.",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.5",
        "ext-json": "*",
        "symfony/config": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/monolog-bundle": "^4.0",
        "doctrine/dbal": "^4.4",
        "symfony/console": "^8.0",
        "symfony/http-foundation": "^8.0",
        "twig/twig": "^3.23",
        "nesbot/carbon": "^3.11",
        "symfony/routing": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "friendsofphp/php-cs-fixer": "^3.93",
        "phpstan/phpstan": "^2.1",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpdoc-parser": "^2.3",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dragonwize\\DwLogBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "dragonwize",
            "email": "dragonwize@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
